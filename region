CASE WHEN Código ISO de la región='ES-CT' THEN 'Cataluña'
     WHEN Código ISO de la región='ES-VC' THEN 'Valencia'
     WHEN Código ISO de la región='ES-AN' THEN 'Andalucía'
     WHEN Código ISO de la región='ES-PV' THEN 'País Vasco'
     WHEN Código ISO de la región='ES-GA' THEN 'Galicia'
     WHEN Código ISO de la región='ES-CL' THEN 'Castilla-León'
     WHEN Código ISO de la región='ES-AR' THEN 'Aragón'
     WHEN Código ISO de la región='ES-CM' THEN 'Castilla La Mancha'
     WHEN Código ISO de la región='ES-IB' THEN 'Islas Baleares'
     WHEN Código ISO de la región='ES-MD' THEN 'Madrid'
     WHEN Código ISO de la región='ES-CN' THEN 'Canarias'
     WHEN Código ISO de la región='ES-EX' THEN 'Extremadura'
     WHEN Código ISO de la región='ES-CB' THEN 'Cantabria'
     WHEN Código ISO de la región='ES-AS' THEN 'Asturias'
     WHEN Código ISO de la región='ES-MC' THEN 'Región de Murcia'
     WHEN Código ISO de la región='ES-NC' THEN 'Navarra'
     WHEN Código ISO de la región='ES-CE' THEN 'Ceuta'
     WHEN Código ISO de la región='ES-ML' THEN 'Melilla'
     WHEN Código ISO de la región='ES-RI' THEN 'La Rioja'
END
